<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <bean id="student" class="com.pojo.Student" scope="singleton">
   <property name="id" value="30"></property>
   <property name="name" value="pradeep"></property>
   <property name="salary" value="85000.23"></property>
   <property name="email" value="pradeep@kontam"></property>
   </bean>
   

   <alias name="student"  alias="hello"/>
   
  

</beans>